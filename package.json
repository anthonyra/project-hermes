{
  "name": "@gala-games/project-hermes",
  "version": "0.1.0",
  "description": "Product Chaincode",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "start": "node lib/src/cli.js start",
    "clean": "tsc -b --clean",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "lint": "eslint . --ext .ts,.js",
    "fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' 'e2e/**/*.ts' --write",
    "prepublishOnly": "npm run format && npm run build && npm run lint && npm run test",
    "network:start": "galachain network:up -C=product-channel -t=curator -n=basic-product -d=./ --envConfig=./.dev-env",
    "network:prune": "galachain network:prune",
    "network:recreate": "npm run network:prune && rm -rf ./test-network && npm run network:start",
    "test": "jest",
    "test:e2e": "jest --config=e2e/jest.config.js --runInBand",
    "update-snapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "@gala-games/chain-api": "0.4.33",
    "@gala-games/chaincode": "0.4.33",
    "dotenv": "^16.0.1",
    "fabric-contract-api": "2.4.2",
    "fabric-shim": "2.4.2"
  },
  "devDependencies": {
    "@gala-games/chain-cli": "0.4.33",
    "@gala-games/chain-client": "0.4.33",
    "@gala-games/chain-test": "0.4.33",
    "@types/axios": "^0.14.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "axios": "^1.6.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-prettier-config": "^1.0.1",
    "fabric-ca-client": "^2.2.19",
    "jest": "^28.1.1",
    "prettier": "^2.2.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
